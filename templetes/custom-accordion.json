       padding-top: 0;\n        padding-bottom: 2.5em;\n    }\n\n    .tab-images {\n        opacity: 0;\n        visibility: hidden;\n        transition: opacity 0.5s ease, visibility 0s linear 0.5s;\n    }\n    .tab-images.active {\n        opacity: 1;\n        visibility: visible;\n        transition: opacity 0.5s ease;\n    }\n<\/style>\n<script>\n\n document.addEventListener('DOMContentLoaded', function () {\n    const headerSections = document.querySelectorAll('.tab-header');\n    const contentSections = document.querySelectorAll('.tab-content');\n    const images = document.querySelectorAll('.tab-images');\n    const defaultImage = document.querySelector('#img-tab');\n    \n    function showImage(imageId) {\n        images.forEach(image => {\n            image.classList.remove('active');\n        });\n        const imageToShow = document.querySelector(`#${imageId}`);\n        if (imageToShow) {\n            imageToShow.classList.add('active');\n        }\n    }\n    headerSections.forEach(header => {\n        header.addEventListener('click', function () {\n            const target = this.getAttribute('data-target');\n            const contentSection = document.querySelector(`.tab-content[data-content=\"${target}\"]`);\n            \n            if (contentSection.classList.contains('active')) {\n                contentSection.classList.remove('active');\n            } else {\n                contentSections.forEach(section => {\n                    section.classList.remove('active');\n                });\n                contentSection.classList.add('active');\n            }\n        });\n        \n        header.addEventListener('mouseover', function () {\n            const target = this.getAttribute('data-target');\n            showImage(\"img-\"+target);\n        });\n\n        header.addEventListener('mouseout', function () {\n            showImage(defaultImage.id);\n        });\n    });\n});  \n<\/script>"},"elements":[],"isInner":false,"widgetType":"html","elType":"widget"}],"isInner":false,"elType":"container"}],"page_settings":{"hide_title":"yes","background_color":"#000000"},"version":"0.4","title":"custom-accordion","type":"container"}